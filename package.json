{
  "name": "discord-clone",
  "version": "1.0.0",
  "main": "build/main.bundle.js",
  "repository": "https://github.com/Guilospanck/discord-clone.git",
  "author": "Guilherme Rodrigues <guilherme.rp2010@hotmail.com>",
  "scripts": {
    "start": "cd build/ && npx http-server .",
    "start:dev": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:staged": "yarn test -- --findRelatedTests src/",
    "test:cov": "jest --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "check": "npx npm-check -u -s"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.2",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "git-commit-msg-linter": "^3.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.5.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}